{
  "name": "nexusmind",
  "version": "1.0.0",
  "description": "NexusMind - 智能体思维连接平台，基于JS的LLM自主智能体，支持ReAct决策、短期记忆和MCP协议",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node test/test.js",
    "amap": "node init-amap-agent.js",
    "amap-demo": "node amap-example.js",
    "quick-start": "node quick-start.js",
    "multi-mcp": "node examples/multi-mcp-example.js",
    "quick-multi-mcp": "node quick-start-multi-mcp.js",
    "frontend": "cd packages/frontend && npm run dev",
    "backend": "cd packages/backend && npm run dev",
    "backend:enhanced": "cd packages/backend && npm run dev:enhanced",
    "backend:debug": "cd packages/backend && npm run dev:debug",
    "backend:watch": "cd packages/backend && npm run dev:watch",
    "backend:verbose": "cd packages/backend && npm run dev:verbose",
    "dev:all": "concurrently \"npm run backend\" \"npm run frontend\"",
    "dev:all:enhanced": "concurrently \"npm run backend:enhanced\" \"npm run frontend\"",
    "dev:all:debug": "concurrently \"npm run backend:debug\" \"npm run frontend\"",
    "monitor": "cd packages/backend && node scripts/monitor.js",
    "build:frontend": "cd packages/frontend && npm run build",
    "install:all": "npm install && cd packages/frontend && npm install && cd ../backend && npm install",
    "start:all": "concurrently \"cd packages/backend && npm start\" \"cd packages/frontend && npm run preview\"",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist",
    "reset": "npm run clean && npm run install:all"
  },
  "keywords": [
    "llm",
    "agent",
    "autonomous",
    "react",
    "mcp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "node-cache": "^5.1.2",
    "openai": "^4.20.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
